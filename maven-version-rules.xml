        <rule groupId="fish.payara.api" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">([6-9]|[1-9][0-9]+)\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="fish.payara.distributions" comparisonMethod="maven">
